Subject: [PATCH] Update the overlay makefile to compile the new wilink8 and
 tlv overlays.

---
 arch/arm/boot/dts/overlays/Makefile | 2 ++
 1 file changed, 2 insertions(+)

diff --git a/arch/arm/boot/dts/overlays/Makefile b/arch/arm/boot/dts/overlays/Makefile
index 180f8b3..4653da4 100644
--- a/arch/arm/boot/dts/overlays/Makefile
+++ b/arch/arm/boot/dts/overlays/Makefile
@@ -106,6 +106,7 @@ dtbo-$(CONFIG_ARCH_BCM2835) += \
 	sdhost.dtbo \
 	sdio.dtbo \
 	sdio-1bit.dtbo \
+	sdio-wlink8.dtbo \
 	sdtweak.dtbo \
 	smi.dtbo \
 	smi-dev.dtbo \
@@ -123,6 +124,7 @@ dtbo-$(CONFIG_ARCH_BCM2835) += \
 	superaudioboard.dtbo \
 	sx150x.dtbo \
 	tinylcd35.dtbo \
+	tlv320aic3x-simple.dtbo \
 	uart0.dtbo \
 	uart1.dtbo \
 	upstream.dtbo \
